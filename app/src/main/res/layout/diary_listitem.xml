<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <!-- <ImageView
        android:id="@+id/imageView9"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_gravity="center_vertical|center_horizontal"
android:layout_marginLeft="5dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/one" />-->

    <android.support.v7.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="4dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="8dp"
        android:elevation="12dp"
        android:innerRadius="0dp"
        android:shape="ring"
        android:thicknessRatio="1.9"
        app:cardCornerRadius="20dp"


        >


        <RelativeLayout
            android:id="@+id/forbackgroundrelativeLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            >


            <android.support.constraint.ConstraintLayout
                android:id="@+id/linearLayout3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:orientation="vertical">


                <!--  android:textSize="@dimen/nickdate" -->

                <TextView
                    android:id="@+id/dateofday"
                    android:layout_width="wrap_content"

                    android:layout_height="wrap_content"

                    android:textColor="#A9A9A9"
                    android:textSize="@dimen/nickdate"
                    app:layout_constraintStart_toStartOf="@+id/diary_entry"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/diary_entry"
                    android:layout_width="0dp"

                    android:layout_height="wrap_content"

                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:maxLines="3"
                    android:textSize="@dimen/entry"
                    app:layout_constraintBottom_toTopOf="@+id/favoritesImage"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/dateofday"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/diary_location"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"


                    android:layout_marginEnd="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="28dp"
                    android:textSize="@dimen/location"
                    android:textStyle="italic"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/moodImage"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/diary_entry" />

                <ImageView
                    android:id="@+id/dreamImage"
                    android:layout_width="36dp"
                    android:layout_height="21dp"

                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    />

                <ImageView

                    android:id="@+id/favoritesImage"
                    android:layout_width="30dp"
                    android:layout_height="18dp"


                    android:layout_marginEnd="8dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:srcCompat="@drawable/like" />


                <ImageView

                    android:id="@+id/locationImage"
                    android:layout_width="30dp"
                    android:layout_height="18dp"


                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/favoritesImage"
                    app:srcCompat="@drawable/location1" />


                <ImageView

                    android:id="@+id/moodImage"
                    android:layout_width="30dp"
                    android:layout_height="18dp"


                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/locationImage" />




                <!--
                                 <ImageView
                                    android:id="@+id/YoutubeImage"
                                    android:layout_width="30dp"
                                    android:layout_height="18dp"
                                    app:layout_constraintBottom_toBottomOf="parent"

                                    app:layout_constraintEnd_toStartOf="@+id/moodImage" />
                                -->

            </android.support.constraint.ConstraintLayout>


        </RelativeLayout>


    </android.support.v7.widget.CardView>


    <View
        android:id="@+id/view1"
        android:layout_width="2dp"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:layout_gravity="center_horizontal"
        android:background="#D3D3D3"

        />


    <ImageView
        android:id="@+id/imageViewPicture"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_gravity="center_vertical|center_horizontal"
        android:foregroundGravity="center_vertical|center_horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/view2"
        android:layout_width="2dp"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:layout_gravity="center_horizontal"
        android:background="#D3D3D3"

        />
</LinearLayout>